<?xml version="1.0" encoding="UTF-8"?>
<dokmap version="1.1">
	<meta name="Test Map"
	      authors="SSSS"
	      description="Test layout to test the layout command"/>
		  
	<layout map="*" mode="Team" players="2, 3, 4, 5, 6">
		<spawns>
			<!-- team 1 -->
			<spawn team="0" index="0" x="0" y="0" angle="0"/>
			<spawn team="0" index="1" x="0" y="0" angle="0"/>
			<spawn team="0" index="2" x="0" y="0" angle="0"/>

			<!-- team 2 -->
			<spawn team="1" index="0" x="0" y="0" angle="0"/>
			<spawn team="1" index="1" x="0" y="0" angle="0"/>
			<spawn team="1" index="2" x="0" y="0" angle="0"/>
		</spawns>
		
		<resources> 
			<resource x="0" y="0" type="cu" amount="99999" collectors="99"/>
			<resource x="2000" y="0" type="ru" amount="99999" collectors="99"/>
			<resource x="0" y="2000" type="cu" amount="99999" collectors="99"/>
			<resource x="2000" y="2000" type="ru" amount="99999" collectors="99"/>
		</resources>
		
		<artifacts>
			<artifact x="1000" y="1000"/>
		</artifacts>
		
		<ezs>
			<ez team="0" x="1000" y="1000" radius="500"/>
			<ez team="1" x="1000" y="1000" radius="500"/>
		</ezs>
		
		<units>
			<!-- team 1 -->
			<unit team="0" index="0" x="1000" y="0" angle="0" type="C_Baserunner"/>
			<unit team="0" index="1" x="1000" y="400" angle="60" type="C_Baserunner"/>
			<unit team="0" index="2" x="1000" y="800" angle="120" type="C_Baserunner"/>
			
			<!-- team 2 -->
			<unit team="1" index="0" x="1000" y="1200" angle="180" type="C_Baserunner"/>
			<unit team="1" index="1" x="1000" y="1600" angle="240" type="C_Baserunner"/>
			<unit team="1" index="2" x="1000" y="2000" angle="300" type="C_Baserunner"/>
		</units>
	</layout>
	
	<layout map="*" mode="FFA" players="2, 3, 4, 5, 6">
		<spawns>
			<!-- team 1 -->
			<spawn team="0" x="0" y="0" angle="0"/>
			<spawn team="1" x="0" y="0" angle="0"/>
			<spawn team="2" x="0" y="0" angle="0"/>
			<spawn team="3" x="0" y="0" angle="0"/>
			<spawn team="4" x="0" y="0" angle="0"/>
			<spawn team="5" x="0" y="0" angle="0"/>
		</spawns>
		
		<resources> 
			<resource x="0" y="0" type="cu" amount="99999" collectors="99"/>
			<resource x="2000" y="0" type="ru" amount="99999" collectors="99"/>
			<resource x="0" y="2000" type="cu" amount="99999" collectors="99"/>
			<resource x="2000" y="2000" type="ru" amount="99999" collectors="99"/>
		</resources>
		
		<units>
			<unit team="0" x="1000" y="0" angle="0" type="C_Baserunner"/>
			<unit team="1" x="1000" y="400" angle="60" type="C_Baserunner"/>
			<unit team="2" x="1000" y="800" angle="120" type="C_Baserunner"/>
			<unit team="3" x="1000" y="1200" angle="180" type="C_Baserunner"/>
			<unit team="4" x="1000" y="1600" angle="240" type="C_Baserunner"/>
			<unit team="5" x="1000" y="2000" angle="300" type="C_Baserunner"/>
		</units>
